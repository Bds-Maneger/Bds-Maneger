{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "shelljs": true
    },
    "globals": {
        "bds_log_string": "writable",
        "bds_server_string": "writable",
        "bds_control": "readonly",
        "bds_maneger_api": "readonly",
        "bds_stop": "readonly",
        "bds_start": "readonly",
        "bds_command": "readonly",
        "bds_kill": "readonly",
        "bds_detect": "readonly",
        "bds_app_version": "readonly",
        "bds_settings": "readonly",
        "bds_download": "readonly",
        "bds_backup": "readonly",
        "bds_drive_backup": "readonly",
        "electron_locale_system": "readonly",
        "themes_list": "readonly",
        "serverstated": "writable"
    },
    "ignorePatterns": [
        "node_modules",
        ".github",
        ".vscode",
        ".devcontainer",
        "test",
        "bds_pages"
    ],
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "eqeqeq": 1
    }
}
