{
  "name": "bds-maneger-gui_for-windows",
  "version": "1.0.0",
  "description": "Minimal maneger for Minecraf Bedrock Server for Windows",
  "main": "index.js",
  "scripts": {
    "test": "electron .",
    "build-win": "electron-builder"
  },
  "build": {
    "appId": "com.Sirherobrine23.BDS-Gui",
    "icon": "pages/assents/mcpe.png",
    "asar": "true",
    "win": {
      "target": [
        "msi",
        "appx"
      ]
    },
    "msi": {
      "menuCategory": "Sirherobrine23"
    },
    "appx": {
      "displayName": "BDS Manger for Windows (APPX)",
      "applicationId": "BDSM12",
      "identityName": "BDSMANEGER"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sirherobrine23/bds-maneger-gui_for-windows.git"
  },
  "keywords": [
    "minecraft",
    "bds",
    "bds-server",
    "Windows"
  ],
  "author": "Matheus Sampaio Queiroga (Sirherobrine23)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sirherobrine23/bds-maneger-gui_for-windows/issues"
  },
  "homepage": "https://github.com/Sirherobrine23/bds-maneger-gui_for-windows#readme",
  "dependencies": {
    "decompress-zip": "^0.2.2",
    "electron-is": "^3.0.0"
  },
  "devDependencies": {
    "electron": "^11.0.1",
    "electron-builder": "^22.9.1"
  }
}
